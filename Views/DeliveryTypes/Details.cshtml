@model HSHDelivery.Models.DeliveryType

@{
    ViewData["Title"] = "Details";
}


<br />
<h2>Delivery Type Details</h2>
<br />


<div class="form-border">
    <fieldset disabled>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="DeliveryTypeName" class="custom-label"></label>
            </div>
            <div class="col-4">
                <input asp-for="DeliveryTypeName" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="DeliveryTypeStatus" class="custom-label"></label>
            </div>
            <div class="col-4">
                <input asp-for="DeliveryTypeStatus" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="CreateDate" class="custom-label"></label>
            </div>
            <div class="col-4">
                <input asp-for="CreateDate" class="form-control" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="CreateBy" class="custom-label"></label>
            </div>
            <div class="col-4">
                <input asp-for="CreateBy" class="form-control" />
            </div>
        </div>
    </fieldset>



    <div class="form-group">
        <a asp-action="Edit" asp-route-id="@Model.DeliveryTypeId" class="btn btn-primary">Edit</a>
        <a asp-action="Index" class="btn btn-success">Back to List</a>
    </div>
</div>
